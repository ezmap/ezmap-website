APP_NAME="EZ Map"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=ezmap
DB_USERNAME=root
DB_PASSWORD=

CACHE_STORE=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Mail — used for feedback/bug report forms
MAIL_MAILER=mailgun
MAILGUN_DOMAIN=
MAILGUN_SECRET=

# Snazzy Maps — theme data
SNAZZY_KEY=

# Language detection
LANG_DETECTOR_AUTODETECT=true
LANG_DETECTOR_DRIVER="browser"
LANG_DETECTOR_SEGMENT=0
LANG_DETECTOR_COOKIE=locale
LANG_DETECTOR_LANGUAGES="en, en_GB, en_US:en"

# Google reCAPTCHA v3 — protects feedback and registration forms
# Get keys at: https://www.google.com/recaptcha/admin (choose v3)
RECAPTCHA_V3_SITE_KEY=
RECAPTCHA_V3_SECRET_KEY=

# Google Maps API key — required for map preview and generated code
# Get a key at: https://console.cloud.google.com/apis/credentials
GOOGLE_MAPS_API_KEY=
